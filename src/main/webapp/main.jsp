<script type="text/javascript">
function modpassword(){
	var diag = new top.Dialog();
	diag.Title = "修改密码";
	diag.Width=500;
	diag.Height=400;
	diag.URL = "login!getaccount.action";
	diag.OKEvent = function(){
		diag.innerFrame.contentWindow.document.all("Submit").click();
		diag.close();
	}
	diag.show();
}
</script>
